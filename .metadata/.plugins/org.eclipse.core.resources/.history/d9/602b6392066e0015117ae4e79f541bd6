	static String convet(int num)
	{
		if(num >= 0 && num <= 9)
		{
			return String.valueOf((char)('0' + num));
		
		}
		else if(num >= 10)
		{
			return String.valueOf((char)('A' + num - 10));
		}
		else
		 return null;
		
	}
	
	
