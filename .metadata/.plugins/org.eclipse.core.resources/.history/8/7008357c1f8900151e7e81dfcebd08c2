

import java.util.*;

public class ex3 {

	public static int getNumber(char uppercaseLetter)
	{
		
		//如果字母小写，则转化为大写
		if(uppercaseLetter >= 'a' && uppercaseLetter <= 'z')
		{
			uppercaseLetter += 'A' - 'a';
		}
		System.out.println(uppercaseLetter);
		
		
		int number = 1;
		
		int charA = 'A';
		int get = uppercaseLetter;
		
		int sub = get - charA;
		
		if(sub >= 0 && sub <= 2)
		{
			return 2;
		}
		else if(sub <= 5)
		{
			return 3;
		}
		else if(sub <= 8)
		{
			return 4;
				
		}else if(sub <= 11)
		{
			return 5;
				
		}else if(sub <= 14)
		{
			return 6;
				
		}else if(sub <= 18)
		{
			return 7;
				
		}else if(sub <= 21)
		{
			return 8;
				
		}
		else if(sub <= 25)
		{
			return 9;
				
		}
		
		
		
		
		
		return 0;
	}
	
	
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub

		Scanner input = new Scanner(System.in);
		
		
		System.out.println("Enter a String:");
		
		String str = input.next();
		
		getNumber(str.charAt(0));
		
		System.out.println(getNumber(str.charAt(0)));
		
	}

}
