
public class ex4 {

    static boolean isPrime(int n) 
    {  
        if (n <= 1) 
        {  
            return false;  
        }  
        int k = (int) Math.sqrt(n);  
        for (int i = 2; i <= k; i++) 
        {  
            if(n % i == 0) 
            {  
                return false;  
            }  
        }  
        return true;  
    }  

	
	public static void main(String[] args) {
		// TODO Auto-generated method stub

		System.out.println(isPrime(7));
		
		
	}

}
