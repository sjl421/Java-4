	public static void main(String[] args) {
		// TODO Auto-generated method stub
		//Scanner input = new Scanner(System.in);
		//ArrayList<Integer> num = new ArrayList<Integer>();

		Scanner input = new Scanner(System.in);

		ArrayList<Integer> num = new ArrayList<Integer>();
	
		for(int i = 0;i < 9;i++)
		{
			num.add(input.nextInt());
		}
		
		
		//计算di'shi'we
		int num10 = 0;
		for(int i = 0;i < 9;i++)
		{
			num10 = num.get(i) * (i+1);
		}
		num10 = num10 % 11;
		System.out.println(num10);
		
		
		
		

	}

