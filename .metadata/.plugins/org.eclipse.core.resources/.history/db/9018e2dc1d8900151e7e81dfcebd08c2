	{
		if(uppercaseLetter >= 'a' && uppercaseLetter <= 'z')
		{
			uppercaseLetter += 'A' - 'a';
		}
		System.out.println(uppercaseLetter);
		
		
		int number = 1;
		
		int charA = 'A';
		int get = uppercaseLetter;
		
		int sub = get - charA;
		
		if(sub==0)
		{
			
		}
		
		
		
		
		return 0;
	}
	
	
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub

		Scanner input = new Scanner(System.in);
		
		
